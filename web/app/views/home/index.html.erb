
<div class="hero">
  <h1>TuParqueo.com en:</h1>
  <form class="form-inline" method="GET" action="parkings">
    <div class="form-group">
      <input type="text" class="form-control" id="" placeholder="Dirección">
    </div>
    <div class="form-group">
      <input type="date" class="form-control" id="fechaInicio" placeholder="Fecha Inicio">
    </div>
    <div class="form-group">
      <input type="date" class="form-control" id="fechaFin" placeholder="Fecha Fin">
    </div>
    <div class="form-group">
      <select id="district" class="form-control">
        <option>San Isidro</option>
        <option>Surco</option>
        <option>San Miguel</option>
      </select>
    </div>
    <button type="submit" class="btn btn-success">Buscar</button>
  </form>
  <br>
  </br>
</div>


<div class="container" id="list-thumbnail">
  <div class="row text-center">
    <div class="col-md-3">
      <a href="#">
        <%= image_tag 'informacion-servicio' , :class=> 'img-responsive' %>
        <div class="caption">
          <h3>Información del servicio</h3>
        </div>                
       </a>
    </div>
            
    <div class="col-md-3">
      <a href="#">
        <%= image_tag 'terminos-y-condiciones' , :class=> 'img-responsive'%>
        <h3>Términos y condiciones de uso</h3>
       </a>
    </div>
    
    <div class="col-md-3">'
      <a href="/home/testimonio">
        <%= image_tag 'testimonios' , :class=> 'img-responsive'%>
        <h3>Testimonios</h3>
     </a>
    </div>  
    <div class="col-md-3">
      <a href="#">
        <%= image_tag 'preguntas-y-respuestas' , :class=> 'img-responsive'%>
        <h3>Preguntas y respuestas frecuentes</h3>
       </a>
    </div>            
  </div>
</div>